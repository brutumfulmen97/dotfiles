(defcfg
  process-unmapped-keys yes
)
(defsrc
  ralt 
  caps h j k l d spc
)
(defalias
  ralt-mod (tap-hold 200 200 lmet lmet)
  caps-mod (tap-hold 200 200 esc (layer-toggle arrows))
)
(deflayer base
  @ralt-mod
  @caps-mod h j k l d spc
)
(deflayer arrows
  @ralt-mod
  _ left down up right del ret
)
